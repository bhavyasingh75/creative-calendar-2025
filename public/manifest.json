{
  "manifest_version": 3,
  "name": "Creative Calendar 2025",
  "version": "1.0",
  "description": "A creative calendar for managing tasks and events",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "favicon_io/favicon-16x16.png",
    "32": "favicon_io/favicon-32x32.png",
    "48": "favicon_io/favicon-32x32.png",
    "128": "favicon_io/android-chrome-192x192.png",
    "192": "favicon_io/android-chrome-192x192.png",
    "512": "favicon_io/android-chrome-512x512.png"
  }
}
