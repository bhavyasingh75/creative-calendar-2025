{
  "manifest_version": 3,
  "name": "Creative Calendar 2025",
  "version": "1.0",
  "description": "A creative calendar for managing tasks and events",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon-16.png",
    "32": "icon-32.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
}
